ENV=development
APP_TITLE=AI REVENUE MACHINE
APP_VERSION=0.1.0
LOG_LEVEL=INFO

# DB
DATABASE_URL=postgresql://user:password@host:5432/dbname
DB_POOL_MIN_SIZE=1
DB_POOL_MAX_SIZE=10

# CORS (dev)
ALLOW_ORIGINS=http://localhost:3000,http://localhost:5173
ALLOW_CREDENTIALS=true

# Auth
N8N_API_KEY=changeme
AUTH_BYPASS_HEALTH=true

# Agent / LLM
DEBUG_AGENT_LOGS=true
DEFAULT_MODEL_NAME=google/gemini-2.5-flash
DEFAULT_USE_TAVILY=false
USE_OPENROUTER=true

# Studio override (opcional)
STUDIO_MODEL_NAME=oogle/gemini-2.5-flash
STUDIO_USE_TAVILY=true

OPENROUTER_API_KEY=your-openrouter-api-key
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MAX_TOKENS=1024
OPENAI_API_KEY=your-openai-api-key
OPENAI_BASE_URL=https://api.openai.com/v1
